for fizzbuzz in range(1, 100):

    # number divisible by 15,(divisible
    # by both 3 & 5), print 'fizzbuzz'
    # in place of the number
    if fizzbuzz % 15 == 0:
        print("fizzbuzz")
        continue

    #number divisible by 3, print 'fizz'
    # in place of the number
    elif fizzbuzz % 3 == 0:
        print("fizz")
        continue

    # number divisible by 5,
    # print 'buzz' in
    # place the number
    elif fizzbuzz % 5 == 0:
        print("buzz")

    # print numbers
